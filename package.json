{
  "name": "swtm",
  "version": "0.0.1",
  "description": "simple working time management",
  "homepage": "https://github.com/skht777/swtm-svelte#readme",
  "bugs": {
    "url": "https://github.com/skht777/swtm-svelte/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skht777/swtm-svelte.git"
  },
  "license": "MIT",
  "author": "skht777",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "vite dev",
    "format": "prettier --write --plugin-search-dir=. .",
    "postinstall": "svelte-kit sync",
    "lint": "prettier --check --plugin-search-dir=. . && eslint .",
    "package": "svelte-kit package",
    "prepare": "husky install",
    "preview": "vite preview",
    "test": "npm run test:ui && npm run test:unit",
    "test:ui": "npx playwright install && cross-env NODE_NO_WARNINGS=1 playwright test",
    "test:unit": "vitest run"
  },
  "dependencies": {
    "d3": "^7.7.0",
    "date-fns": "^2.29.3",
    "dexie": "^3.2.2",
    "tailwindcss": "^3.2.4",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@playwright/test": "^1.29.0",
    "@sveltejs/adapter-static": "^1.0.0",
    "@sveltejs/kit": "^1.0.1",
    "@testing-library/svelte": "^3.2.2",
    "@types/node": "^18.11.16",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "@vitest/ui": "^0.25.8",
    "autoprefixer": "^10.4.13",
    "cross-env": "^7.0.3",
    "eslint": "8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "fake-indexeddb": "^4.0.1",
    "husky": "^8.0.2",
    "jsdom": "^20.0.3",
    "lint-staged": "^13.1.0",
    "npm-check-updates": "^16.6.0",
    "postcss": "^8.4.20",
    "postcss-load-config": "^4.0.1",
    "prettier": "^2.8.1",
    "prettier-plugin-svelte": "^2.9.0",
    "sass": "^1.57.0",
    "sort-package-json": "^2.1.0",
    "svelte": "^3.55.0",
    "svelte-check": "^2.10.2",
    "svelte-preprocess": "^5.0.0",
    "tailwindcss": "^3.2.4",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.1",
    "vitest": "^0.25.8"
  },
  "overrides": {
    "vite": "$vite"
  }
}
